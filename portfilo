1st.css
body {
    font-family: Arial, sans-serif;
    margin: 0;
    padding: 0;
    background-color: #f4f4f4;
}

header {
    background-color: #333;
    color: white;
    padding: 20px;
    text-align: center;
    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}

main {
    padding: 20px;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}

.profiles {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    gap: 20px; /* Space between cards */
}

.profiles a {
    text-decoration: none;
    color: black;
    display: block;
    width: 250px;
    background-color: white;
    border-radius: 10px;
    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
    transition: transform 0.3s ease, box-shadow 0.3s ease;
    padding: 20px;
    text-align: center;
    font-size: 18px;
}

.profiles a:hover {
    transform: scale(1.05);
    box-shadow: 0 6px 8px rgba(0, 0, 0, 0.2);
}

.profiles a::before {
    content: '';
    display: block;
    width: 100px;
    height: 100px;
    margin: 0 auto;
    background: url('placeholder-image.jpg') no-repeat center center;
    background-size: cover;
    border-radius: 50%;
    margin-bottom: 10px;
}

/* Example CSS for a default placeholder image */
.profiles a:nth-of-type(1)::before {
    background-image: url('img1.png');
}

.profiles a:nth-of-type(2)::before {
    background-image: url('img3.jpg');
}

.profiles a:nth-of-type(3)::before {
    background-image: url('img4.jpg');
}

.profiles a:nth-of-type(4)::before {
    background-image: url('img2.jpg');
}
